{
  "name": "nbinspect",
  "displayName": "Notebook state inspect",
  "description": "Helper extension for notebook state",
  "version": "0.0.25",
  "publisher": "spikey-summers",
  "repository": {
    "type": "git",
    "url": "https://github.com/civvic/nb-bridget.git",
    "directory": "nbinspect-vscode"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yourusername/bridget/issues"
  },
  "homepage": "https://github.com/civvic/bridget/tree/main/nbinspect-vscode#readme",
  "engines": {
    "vscode": "^1.93.0",
    "node": ">=20.17.0"
  },
  "categories": [
    "Notebooks",
    "Other"
  ],
  "activationEvents": [
    "onNotebook:*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nbinspect.testClearOutput",
        "title": "Test Clear Notebook Output"
      }
    ],
    "notebookRenderer": [
      {
        "id": "nbinspect-renderer",
        "displayName": "Notebook state renderer",
        "mimeTypes": [
          "application/x-notebook-state"
        ],
        "entrypoint": "./dist/renderer.js",
        "requiresMessaging": "optional"
      }
    ]
  },
  "scripts": {
    "compile": "node esbuild.js",
    "watch": "node esbuild.js --watch",
    "lint": "eslint .",
    "test": "mocha",
    "test:watch": "mocha --watch"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "chai": "^5.2.0",
    "debug": "^4.4.0",
    "esbuild": "^0.25.0",
    "eslint": "^9.16.0",
    "mocha": "^11.1.0",
    "sinon": "^19.0.2"
  }
}
