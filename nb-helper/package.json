{
  "name": "nb-helper",
  "displayName": "Notebook Helper",
  "description": "Helper extension for notebook state",
  "version": "0.0.17",
  "publisher": "spikey-summers",
  "repository": {
    "type": "git",
    "url": "https://github.com/civvic/nb-bridget.git",
    "directory": "nb-helper"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yourusername/bridget/issues"
  },
  "homepage": "https://github.com/civvic/bridget/tree/main/nb-helper#readme",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Notebooks",
    "Other"
  ],
  "activationEvents": [
    "onNotebook:*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nb-helper.getNoteContent",
        "title": "Get Notebook Content"
      }
    ],
    "notebookRenderer": [
      {
        "id": "nb-helper-renderer",
        "displayName": "Notebook Helper Renderer",
        "mimeTypes": [
          "application/x-notebook-state"
        ],
        "entrypoint": "./renderer.js",
        "requiresMessaging": "optional"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.16.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
